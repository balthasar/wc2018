{
  "name": "wc2018",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "yarn run build && push-dir --dir=dist --branch=gh-pages --cleanup --allow-unclean"
  },
  "dependencies": {
    "push-dir": "^0.4.1",
    "vue": "^2.5.16",
    "vue-loading-overlay": "^2.0.4",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.2",
    "@vue/cli-service": "^3.0.0-rc.2",
    "@vue/eslint-config-standard": "^3.0.0-rc.2",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "space-before-function-paren": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
